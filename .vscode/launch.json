    "version": "0.2.0",
    "configurations": [
        {
            "name": "Normal Mode",
            "type": "node",
            "request": "launch",
            "runtimeArgs": ["-r", "ts-node/register"],
            "args": [
                "${workspaceFolder}/src/debug.ts",
                "pr",
                "--browser",
                "--user",
                "protoEvangelion",
                "--repo",
                "gh",
                "--number",
                "97"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Testing Mode",
            "type": "node",
            "request": "launch",
            "env": { "NODE_ENV": "testing", "GH_USER": "protoEvangelion", "GH_TOKEN": "0001" },
            "runtimeArgs": ["-r", "ts-node/register"],
            "args": [
                "${workspaceFolder}/src/debug.ts",
                "pr",
                "--browser",
                "--user",
                "protoEvangelion",
                "--repo",
                "gh",
                "--number",
                "97"
            ],
            "console": "integratedTerminal"
        }
    ]
}
