{
    "name": "gh",
    "description": "GitHub command line tools.",
    "version": "1.13.1",
    "homepage": "http://nodegh.io",
    "author": {
        "name": "Eduardo Lundgren",
        "email": "eduardolundgren@gmail.com",
        "web": "http://eduardo.io",
        "twitter": "eduardolundgren"
    },
    "contributors": [
        {
            "name": "Zeno Rocha",
            "email": "zno.rocha@gmail.com",
            "web": "http://zenorocha.com",
            "twitter": "zenorocha"
        },
        {
            "name": "Ryan Garant",
            "email": "ryantgarant@gmail.com",
            "web": "https://github.com/protoEvangelion"
        }
    ],
    "license": "BSD-3-Clause",
    "repository": {
        "type": "git",
        "url": "https://github.com/node-gh/gh.git"
    },
    "bugs": {
        "url": "https://github.com/node-gh/gh/issues"
    },
    "keywords": [
        "git",
        "github",
        "external",
        "commands",
        "helpers"
    ],
    "bin": {
        "gh": "bin/gh.js"
    },
    "release": {
        "branch": "master",
        "debug": true
    },
    "scripts": {
        "commit": "git-cz",
        "ci": "gulp ci",
        "format": "prettier-standard 'lib/*.js' && prettier-standard 'lib/**/*.js' && prettier-standard 'bin/*.js'",
        "prepare": "babel lib/rest-api-client.js --out-file lib/rest-api-client.js",
        "precommit": "lint-staged",
        "test": "gulp test"
    },
    "czConfig": {
        "path": "node_modules/cz-conventional-changelog"
    },
    "lint-staged": {
        "*.js": [
            "prettier-standard",
            "git add"
        ]
    },
    "dependencies": {
        "async": "^1.5.0",
        "babel-polyfill": "^6.0.16",
        "colors": "^1.1.2",
        "github": "^0.2.4",
        "handlebars": "^4.0.4",
        "inquirer": "^6.2.0",
        "lodash": "^4.17.10",
        "moment": "^2.20.1",
        "nopt": "^3.0.4",
        "opn": "^5.3.0",
        "request": "^2.83.0",
        "truncate": "^2.0.0",
        "update-notifier": "^0.5.0",
        "userhome": "^1.0.0",
        "which": "^1.2.0",
        "wordwrap": "^1.0.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-eslint": "^8.2.6",
        "commitizen": "^2.10.1",
        "cz-conventional-changelog": "^2.1.0",
        "eslint": "^4.0.0",
        "eslint-config-esnext": "^3.0.0",
        "eslint-config-prettier": "^3.0.1",
        "eslint-config-recommended": "^3.0.0",
        "eslint-plugin-prettier": "^2.6.2",
        "gulp": "^4.0.0",
        "gulp-complexity": "^0.3.2",
        "gulp-istanbul": "^1.1.3",
        "gulp-mocha": "^6.0.0",
        "husky": "^0.14.3",
        "lint-staged": "^6.0.0",
        "prettier": "^1.9.2",
        "prettier-standard": "^8.0.1",
        "rewire": "^2.3.4",
        "semantic-release": "^15.9.9",
        "travis-deploy-once": "^5.0.3"
    },
    "engines": {
        "node": ">=0.12"
    },
    "preferGlobal": true
}
